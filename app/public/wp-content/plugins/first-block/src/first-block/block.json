{
	"$schema": "https://schemas.wp.org/trunk/block.json",
	"apiVersion": 3,
	"name": "create-block/first-block",
	"version": "0.1.0",
	"title": "First Block",
	"category": "media",
	"icon": "admin-plugins",
	"description": "Code is poetry",
	"example": {},
	"attributes": {
		"content": {
			"type": "string",
			"default": "Hello World"
		}
	},
	"supports": {
		"html": false,
		"anchor": true,
		"align": true,
		"color": {
			"background": true,
			"text": true,
			"link": true,
			"gradients": true
		},
		"spacing": {
			"margin": true,
			"padding": true,
			"blockGap": true
		},
		"typography": {
			"fontSize": true,
			"lineHeight": true,
			"fontFamily": true,
			"textTransform": true,
			"textDecoration": true,
			"fontWeight": true
		}
	},
	"textdomain": "first-block",
	"editorScript": "file:./index.js",
	"editorStyle": "file:./index.css",
	"style": "file:./style-index.css",
	"viewScript": "file:./view.js"
}
